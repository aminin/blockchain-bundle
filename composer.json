{
    "name": "aminin/blockchain-bundle",
    "description": "Blockchain API integration for Symfony 2/3",
    "repositories": [{ "type": "composer", "url": "https://aminin.github.io/satis" }],
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Minin",
            "email": "anton.a.minin@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "require": {
        "symfony/framework-bundle": "^2.8|^3.0|^4.0",
        "symfony/yaml": "*",
        "blockchain/blockchain": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Ami\\BlockchainBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
